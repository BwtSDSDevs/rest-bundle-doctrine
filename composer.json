{
    "name": "dontdrinkandroot/rest-bundle",
    "description": "Symfony bundle that enables you to easily expose your Doctrine Entities via REST",
    "type": "symfony-bundle",
    "keywords": [
        "REST",
        "Doctrine"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/security": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/finder": "^4.4",
        "symfony/serializer": "^4.1",
        "doctrine/annotations": "^1.2",
        "jms/metadata": "^2.6"
    },
    "require-dev": {
        "symfony/yaml": "^4.4",
        "liip/test-fixtures-bundle": "^2.2",
        "phpunit/phpunit": "^7.0",
        "symfony/dom-crawler": "^4.4",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "symfony/security-bundle": "^4.4",
        "fzaninotto/faker": "^1.6",
        "ramsey/uuid": "^3.5",
        "symfony/browser-kit": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\RestBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    }
}
