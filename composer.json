{
    "name": "sdsdev/rest-bundle-doctrine",
    "description": "Symfony bundle that enables you to easily expose your Doctrine Entities via REST",
    "type": "symfony-bundle",
    "keywords": [
        "REST",
        "Doctrine"
    ],
    "license": "Apache-2.0",
    "version": "0.1",
    "authors": [
        {
            "name": "Dominik Ahammer",
            "email": "ahammer.dominik@gmail.com"
        },
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "jms/metadata": "^2.6",
        "symfony/expression-language": "7.1.*",
        "symfony/finder": "7.1.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/property-access": "7.1.*",
        "symfony/security-core": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/validator": "7.1.*"
    },
    "require-dev": {
        "brianium/paratest": "^6.10",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "liip/test-fixtures-bundle": "^2.2",
        "phpunit/phpunit": "^9.5.11",
        "symfony/browser-kit": "7.1.*",
        "symfony/dom-crawler": "7.1.*",
        "symfony/phpunit-bridge": "7.1.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/uid": "7.1.*",
        "symfony/yaml": "7.1.*"
    },
    "autoload": {
        "psr-4": {
            "SdsDev\\RestBundleDoctrine\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "require": "7.1.*",
            "allow-contrib": true
        },
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
