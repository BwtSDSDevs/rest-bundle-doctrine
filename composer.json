{
    "name": "dontdrinkandroot/rest-bundle",
    "description": "Symfony bundle that enables you to easily expose your Doctrine Entities via REST",
    "type": "symfony-bundle",
    "keywords": [
        "REST",
        "Doctrine"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/annotations": "^1.2",
        "jms/metadata": "^2.6",
        "symfony/finder": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/property-access": "^5.4",
        "symfony/security-core": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/validator": "^5.4"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "liip/test-fixtures-bundle": "^2.2",
        "phpunit/phpunit": "^9.5.11",
        "symfony/browser-kit": "^5.4",
        "symfony/dom-crawler": "^5.4",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/uid": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\RestBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    }
}
